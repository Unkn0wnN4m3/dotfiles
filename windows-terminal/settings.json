{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": [
        {
            "command": {
                "action": "copy",
                "singleLine": false
            },
            "keys": "ctrl+shift+c"
        },
        {
            "command": "paste",
            "keys": "ctrl+shift+v"
        },
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": {
                "action": "splitPane",
                "split": "down",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+("
        },
        {
            "command": {
                "action": "splitPane",
                "split": "right",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+)"
        }
    ],
    "initialPosition": "5,5",
    "copyFormatting": "none",
    "copyOnSelect": false,
    "profiles": {
        "defaults": {
            "bellStyle": "taskbar",
            "font": {
                "face": "JetBrains Mono",
                "size": 12
            },
            "opacity": 100,
            "padding": "2",
            "useAcrylic": false
        }
    },
